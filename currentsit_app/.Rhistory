text_analysis_game <- function(sentence) {
# Calculate number of words
words <- strsplit(sentence, "\\s+")[[1]]
number_of_words <- length(words)
# Calculate number of characters
num_characters <- nchar(gsub("\\s+", "", sentence))
# Calculate average word length
avg_word_length <- mean(nchar(words))
# Determine communication skill level
communication_skill_level <- ifelse(number_of_words >= 10, "Advanced",
ifelse(number_of_words >= 5, "Intermediate", "Beginner"))
# Print statistics and communication skill level
cat("\nStatistics:\n")
cat("Number of words:", number_of_words, "\n")
cat("Number of characters:", num_characters, "\n")
cat("Average word length:", round(avg_word_length, 2), "\n")
cat("\nCommunication Skill Level:", communication_skill_level, "\n")
}
